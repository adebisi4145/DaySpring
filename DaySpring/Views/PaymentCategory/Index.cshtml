@model DaySpring.ViewModels.PaymentCategoriesResponseModel

@{
    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
    ViewData["Title"] = "Index";
}

<div class="card-header" style="background-color: whitesmoke;">
    <div class="card-title">
        <h1 class="text-center">Payment Categories</h1>
    </div>
</div>

<table class="table">
    <thead>
        <tr>
            <th>
                Name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model.Data)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => category.Name)
                </td>

            </tr>
        }
    </tbody>
</table>
<button class="btn btn-primary offset-10 " style="background-color: whitesmoke;"><a asp-action="Create">Add</a></button>