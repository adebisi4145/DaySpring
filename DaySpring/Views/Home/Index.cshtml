@{
    ViewData["Title"] = "Home Page";
}



<header class="masthead">
    <div class="container px-4 px-lg-5 h-100">
        <div class="row gx-4 gx-lg-5 h-100 align-items-center justify-content-center text-center">
            <div class="col-lg-8 align-self-end">
                <h1 class="text-white font-weight-bold">I Love Where I Worship</h1>
                <hr class="divider" />
            </div>
            <div class="col-lg-8 align-self-baseline">
                <p class="text-white-75 mb-5"> Discovering Purpose, Building Potentials and Fulfilling Dreams</p>
                <a class="btn btn-primary btn-xl" href="#about">Find Out More</a>
            </div>
        </div>
    </div>
</header>
<section class="page-section bg-primary" id="about">
    <div class="container px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
            <div class="col-lg-8 text-center">
                <h2 class="text-white mt-0">Who are We?</h2>
                <hr class="divider divider-light" />
                <p class="text-white-75 mb-4">We are a church with vision to raise  men and women that are highly motivated and responsible to God, their families and society relating together and worshipping God in an atmosphere of love and opportunity for purpose discovery and potential building.</p>
                <p class="text-white-75 mb-4">We are a contemporary, but not compromising church, with beautiful ambience, quality service delivery, great worship experience, purpose driven, building capacity for leadership and management with commitment to love, excellence and accountability.</p>
                <a class="btn btn-light btn-xl" asp-controller="Member" asp-action="Register">Get Started!</a>
            </div>
        </div>
    </div>
</section>
<section class="page-section" id="services">
    <div class="container px-4 px-lg-5">
        <h2 class="text-center mt-0">At Your Service</h2>
        <hr class="divider" />
        <div class="row gx-4 gx-lg-5">
            <div class="col-lg-3 col-md-6 text-center">
                <div class="mt-5">
                    <div class="mb-2"><i class="bi-gem fs-1 text-primary"></i></div>
                    <h3 class="h4 mb-2">Library</h3>
                    <p class="text-muted mb-0">Our themes are updated regularly to keep them bug free!</p>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 text-center">
                <div class="mt-5">
                    <div class="mb-2"><i class="bi-laptop fs-1 text-primary"></i></div>
                    <h3 class="h4 mb-2">Sermons</h3>
                    <p class="text-muted mb-0">We give you access to gospel messages and life transforming sermon.</p>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 text-center">
                <div class="mt-5">
                    <div class="mb-2"><i class="bi-globe fs-1 text-primary"></i></div>
                    <h3 class="h4 mb-2">Give Online</h3>
                    <p class="text-muted mb-0">Give your seed, tithe, offering... with ease</p>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 text-center">
                <div class="mt-5">
                    <div class="mb-2"><i class="bi-heart fs-1 text-primary"></i></div>
                    <h3 class="h4 mb-2">Announcement</h3>
                    <p class="text-muted mb-0">Be the first to know what's happening in church</p>
                </div>
            </div>
        </div>
    </div>
</section>
